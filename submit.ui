   <property name="styleSheet">
    <string notr="true">/* 基础日历控件样式 */
QCalendarWidget {
    border: 1px solid #e0e0e0;
    border-radius: 4px;
    background-color: #fff8e1;  /* 暖色背景 */
}

/* 导航栏区域 */
QCalendarWidget #qt_calendar_navigationbar {
    background-color: #f5f5f5;
    border-bottom: 1px solid #ddd;
}

/* 导航按钮 - 确保文字可见 */
QCalendarWidget QToolButton {
    background-color: #f5f5f5;
    border: 1px solid #ddd;
    border-radius: 3px;
    padding: 3px;
    color: #555;
    min-width: 60px;  /* 确保按钮有足够空间显示文字 */
}

QCalendarWidget QToolButton:hover {
    background-color: #e0e0e0;
}

/* 月份/年份菜单 */
QCalendarWidget QMenu {
    background-color: #fff;
    border: 1px solid #ddd;
}

/* 星期标题行 */
QCalendarWidget QWidget#qt_calendar_weekdaybar {
    background-color: transparent;
    qproperty-weekdayTextFormat: &quot;ddd&quot;;
}

QCalendarWidget QWidget#qt_calendar_weekdaybar QLabel {
    font-weight: bold;
    color: #333;
    background-color: transparent;
}

/* 日期表格视图 */
QCalendarWidget QTableView {
    alternate-background-color: #fffaf0;
    background-color: #fff8e1;
    selection-background-color: #ff9966;
    selection-color: white;
}

/* 表头样式 */
QCalendarWidget QHeaderView {
    background-color: transparent;
    border: none;
}

QCalendarWidget QHeaderView::section {
    background-color: transparent;
    border: none;
    color: #333;
    font-weight: bold;
}

/* 日期单元格 */
QCalendarWidget QAbstractItemView:enabled {
    color: #333;
    font-weight: medium;
}

QCalendarWidget QAbstractItemView:disabled {
    color: #aaaaaa;
}

/* 确保当前日期高亮 */
QCalendarWidget QAbstractItemView:selected {
    background-color: #ff9966;
    color: white;
    border-radius: 3px;
}

/* 修复日历格子线 */
QCalendarWidget QTableView::item {
    border: 1px solid rgba(0,0,0,0.05);
}
